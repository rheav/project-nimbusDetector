{
  "manifest_version": 3,
  "name": "nimbusDetector",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },

  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": ["*://*/"],
  "icons": {
    "16": "src/assets/logo/icon16.png",
    "32": "src/assets/logo/icon32.png",
    "48": "src/assets/logo/icon48.png",
    "128": "src/assets/logo/icon128.png"
  }
}
